<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="5.0" ExtensionBundleId="com.beakus.AERender" ExtensionBundleVersion="1.4.1"
		ExtensionBundleName="AERender" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.beakus.AERender" Version="1.4.1" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="AEFT" Version="[12.0,15.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="5.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>
    
	<DispatchInfoList>
		<Extension Id="com.beakus.AERender">
			<DispatchInfo >
				<Resources>
                    <MainPath>./index.html</MainPath>
                    <ScriptPath>./jsx/AERender.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
                    <StartOn>
                        <Event>applicationActivate</Event>
                    </StartOn>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>BEAKUS AERender</Menu>
					<Geometry>
						<Size>
							<Height>212</Height>
							<Width>175</Width>
						</Size>
<!--
                    <MinSize>
                            <Height>225</Height>
                            <Width>150</Width>
                        </MinSize>
                        <MaxSize>
                            <Height>225</Height>
                            <Width>150</Width>
                        </MaxSize>             
-->
					</Geometry>
					<Icons>
						<Icon Type="Normal">./icons/iconNormal.png</Icon>
						<Icon Type="RollOver">./icons/iconRollover.png</Icon>
						<Icon Type="Disabled">./icons/iconDisabled.png</Icon>
						<Icon Type="DarkNormal">./icons/iconDarkNormal.png</Icon>
						<Icon Type="DarkRollOver">./icons/iconDarkRollover.png</Icon>
					</Icons>					
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>